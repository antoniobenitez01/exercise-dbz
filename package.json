{
  "name": "dbz",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:href": "ng build --base-href ./",
    "build:github": "npm run build:href && npm run delete:docs && npm run copy:dist && npm run git:auto",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "delete:docs": "del docs",
    "copy:dist": "copyfiles dist/dbz/browser/* ./docs -f",
    "git:add":"git add .",
    "git:commit":"git commit -m \"Automatic Angular Push\"",
    "git:push":"git push -u origin main",
    "git:auto":"npm run git:add && npm run git:commit && npm run git:push"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.10",
    "@angular/cli": "^20.3.10",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^7.0.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
